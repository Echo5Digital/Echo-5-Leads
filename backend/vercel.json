{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/sla-check",
      "schedule": "0 */4 * * *"
    },
    {
      "path": "/api/cron/meta-fetch",
      "schedule": "*/10 * * * *"
    }
  ]
}
